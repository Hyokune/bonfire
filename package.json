{
  "name": "bonfire",
  "version": "1.0.0",
  "description": "A Dark Souls tool to help backup saves",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "react:dev": "webpack serve --mode=development",
    "electron:dev": "nodemon",
    "dev": "concurrently --kill-others \"npm run react:dev\" \"npm run electron:dev\"",
    "start": "npm run build && electron dist/main.js",
    "lint": "eslint --parser-options project:tsconfig.json --ext ts,js src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hyokune/bonfire.git"
  },
  "author": "Hyokune",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Hyokune/bonfire/issues"
  },
  "homepage": "https://github.com/Hyokune/bonfire#readme",
  "devDependencies": {
    "@types/react": "18.0.1",
    "@types/react-dom": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.19.0",
    "@typescript-eslint/parser": "5.19.0",
    "concurrently": "7.1.0",
    "electron": "18.0.3",
    "electron-is-dev": "2.0.0",
    "eslint": "8.13.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "goober": "2.1.8",
    "html-webpack-plugin": "5.5.0",
    "nodemon": "2.0.15",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "ts-loader": "9.2.8",
    "typescript": "4.6.3",
    "webpack": "5.72.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.8.1"
  }
}
